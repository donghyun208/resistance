div(ng-show="$ctrl.showEnd")
  h4(ng-show="$ctrl.Game.model.status === 5") Resistance Win
  h4(ng-show="$ctrl.Game.model.status === 6") Spies Win
  button.btn.btn-primary.active(ng-click='$ctrl.leaveGame()') Leave Game
  button.btn.btn-primary(ng-click='$ctrl.newGame()', ng-show="$ctrl.Player.model._id == $ctrl.Game.model.players[0]") New Game
